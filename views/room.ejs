<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"></script>
    <script src="https://kit.fontawesome.com/c939d0e917.js"></script>
    <title>Zoom clone</title>

    <script>
      const ROOM_ID = "<%=roomId%>";
      // console.log("");
    </script>
    <link rel="stylesheet" href="style.css" />

  </head>
  
    <div class="main">
      <div class="main_left">
        <div class="main_videos">
          <div id="video-grid">

          </div>
      </div>
      <div class="main_controls">
        <div class="main_control_block">
             <div class="main_control_button">
              <span>
              <i class="fas fa-microphone" id="mute_button"></i>
              MUTE</span>
  
              <div class="main_control_button">
                <span>
                <i class="fas fa-video" id="stop_Video"></i>
                Stop video</span>
             </div>
             <div class="main_control_button">
              <span>
              <i class="fas fa-shield-alt"></i>
              Security</span>
           </div>
           <div class="main_control_button">
            <span>
            <i class="fas fa-user-friends"></i>
            Participant</span>
         </div>
         <div class="main_control_button">
          <span>
          <i class="fas fa-comment-alt"></i>
          Chat</span>
       </div>
       <div class="main_control_button">
       
        <span style="color: red">Leave Meeting</span>
     </div>
        </div>
      </div>
    </div>
    </div>
      <div class="main_right">
        <div class="main_header">
          <h6>CHAT</h6>
        </div>
        <div class="main_chat_window">
          <ul class="message">

          </ul>
        </div>
        <div class="main_message_container">
          <input id="chat_message" type="text" placeholder="Type message here">
        </div>
      </div>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>    
